{
  "package": "com.example.demo",
  "name": "apex_test",
  "versionName": "1.0.0",
  "versionCode": "1",
  "minPlatformVersion": "1020",
  "icon": "/Common/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "config": {
    "logLevel": "off"
  },
  "router": {
    "entry": "Device",
    "pages": {
      "Device": {
        "component": "index"
      },
      "Person": {
        "component":"index"
      },
      "Notification": {
        "component": "index"
      }
    },
    "widgets": {
      "CardDemo": {
        "name": "CardDemo",
        "description": "快应用卡片展示",
        "component": "index",
        "path": "/CardDemo",
        "minPlatformVersion": 1032,
        "targetManufactorys": [
          "vivo"
        ],
        "features": []
      },
      "card": {
        "name": "ddd",
        "description": "wdd",
        "component": "ddd",
        "path": "/card",
        "features": [
          {
            "name": "system.network"
          }
        ],
        "minPlatformVersion": 1060
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#fefeff",
    "titleBarTextColor": "#414141",
    "pages": {
      "Notification": {
        "titleBar":false
      },
      "Device": {
        "titleBar":false
      },
      "Person":{
        "titleBar":false
      }
    }
  }
}
